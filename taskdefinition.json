{
    "executionRoleArn": "<AWS_EXE_ROLE_ARN>",
    "taskRoleArn": "<AWS_TASK_ROLE_ARN>",
    "memory": "512",
    "cpu": "256",
    "containerDefinitions": [
        {
            "name": "<APP_NAME>",
            "image": "<IMAGE_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "environment": [
                {
                    "name": "PORT",
                    "value": "80"
                },
                {
                    "name": "APP_NAME",
                    "value": "<APP_NAME>"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "family": "<APP_NAME>"
}